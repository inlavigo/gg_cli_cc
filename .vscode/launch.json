  "version": "0.2.0",

  "configurations": [
    {
      "name": "Run aud",
      "type": "dart",
      "request": "launch",
      "program": "aud_cli/bin/aud.dart",
      "args": [
        "createDartPackage",
        "-n",
        "aud_test",
        "-d",
        "Manages colors of Audanika's AbSpace, i.e. key colors, chord type overlays etc",
        "--force"
      ],
      "cwd": "${workspaceFolder}/.."
    },
    {
      "name": "Test Current File",
      "type": "dart",
      "request": "launch",
      "program": "${file}"
    }
  ]
}
